{
  "name": "express-server",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder --dir",
    "pack": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wkeyikai/express-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wkeyikai/express-server/issues"
  },
  "homepage": "https://github.com/wkeyikai/express-server#readme",
  "dependencies": {
    "child_process": "^1.0.2",
    "element-ui": "^2.11.1",
    "express": "^4.17.1",
    "nuxt": "^2.8.1",
    "sqlite3": "^4.0.9"
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^21.2.0"
  },
  "build": {
    "appId": "com.cilent.app.electronbuild",
    "productName": "cc-build",
    "directories": {
      "output": "build"
    },
    "files": [
      "!.nuxt",
      "!package.json",
      "!README.md",
      "node_modules/**",
      "project/nuxt/.nuxt/dist/**",
      "project/nuxt/nuxt.config.js",
      "project/nuxt/index.js",
      "project/index.js",
      "project/index.html",
      "static/**",
      "main.js",
      "config.json"
    ],
    "extraResources": [
      "./project/server/data/"
    ],
    "mac": {
      "icon": "static/favicon.ico"
    },
    "win": {
      "icon": "static/favicon.ico"
    },
    "linux": {
      "icon": "static/favicon.ico"
    }
  }
}
